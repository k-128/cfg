{
    "security.workspace.trust.enabled": true,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.emptyWindow": false,
    "security.workspace.trust.untrustedFiles": "newWindow",
    "extensions.autoUpdate": false,
    "extensions.autoCheckUpdates": true,
    "workbench.colorCustomizations": {
        "sideBar.background": "#212121",
        "sideBar.border": "#2d2d2d",
        "editorBracketMatch.background": "#ffffff18",
        "editorBracketMatch.border": "#ffffff0D",
        "editor.background": "#1b1b1b",
        "editor.selectionBackground": "#135564",
        "editor.selectionHighlightBackground": "#13556477",
        "editor.lineHighlightBackground": "#6161612c",
        "editor.wordHighlightBackground": "#54777730",
        "editor.findMatchBackground": "#21b4b9",
        "editor.findMatchHighlightBackground": "#21b4b977",
        "editorCursor.foreground": "#ffffffbf",
        "terminalCursor.foreground": "#979797bf",
        "notebook.cellBorderColor": "#3D3D3D",
        "notebook.cellEditorBackground": "#282828",
        "notebook.cellHoverBackground": "#212121",
        "notebook.cellInsertionIndicator": "#4A4A4A",
        "notebook.cellStatusBarItemHoverBackground": "#212121",
        "notebook.cellToolbarSeparator": "#3D3D3D",
        "notebook.editorBackground": "#1b1b1b",
        "notebook.focusedCellBackground": "#212121",
        "notebook.focusedCellBorder": "#4A4A4A",
        "notebook.focusedEditorBorder": "#212121",
        "notebook.inactiveFocusedCellBorder": "#282828",
        // "notebook.inactiveSelectedCellBorder": "#ff0000",
        // "notebook.outputContainerBackgroundColor": "#ff0000",
        // "notebook.outputContainerBorderColor": "#ff0000",
        // "notebook.selectedCellBackground": "#ff0000",
        // "notebook.selectedCellBorder": "#ff0000",
        // "notebook.symbolHighlightBackground": "#ff0000"
    },
    "workbench.sideBar.location": "right",
    "workbench.colorTheme": "Sonokai Andromeda",
    "workbench.iconTheme": "eq-material-theme-icons",
    "editor.tokenColorCustomizations": {
        "comments": "#0990ffdc"
    },
    "editor.rulers": [
        80
    ],
    "editor.fontFamily": "FiraCode NF",
    "editor.fontLigatures": true,
    "editor.fontWeight": "500",
    "editor.fontSize": 12,
    "editor.showFoldingControls": "always",
    "editor.minimap.enabled": true,
    "editor.renderWhitespace": "boundary",
    "editor.tabCompletion": "on",
    "editor.quickSuggestionsDelay": 500,
    "editor.scrollBeyondLastLine": true,
    "editor.wrappingStrategy": "simple",
    "editor.wrappingIndent": "same",
    "editor.wordWrap": "off",
    "debug.console.wordWrap": false,
    "diffEditor.wordWrap": "off",
    "breadcrumbs.enabled": false,
    "extensions.ignoreRecommendations": true,
    "extensions.supportUntrustedWorkspaces": {
        "vsciot-vscode.vscode-arduino":         { "supported": "limited" },
        "ms-vscode.cpptools":                   { "supported": "limited" },
        "twxs.cmake":                           { "supported": "limited" },
        "ms-vscode.cmake-tools":                { "supported": "limited" },
        "ms-vscode.vscode-typescript-next":     { "supported": "limited" },
        // "ms-toolsai.jupyter":                   { "supported": "limited" },
        // "ms-toolsai.jupyter-keymap":            { "supported": "limited" },
        "ms-toolsai.jupyter-renderers":         { "supported": "limited" },
        "equinusocio.vsc-material-theme-icons": { "supported": "limited" },
        "ms-python.vscode-pylance":             { "supported": "limited" },
        "ms-python.python":                     { "supported": "limited" },
        "rust-lang.rust-analyzer":              { "supported": "limited" },
        //  "sainnhe.sonokai"                      { "supported": "limited" },
        "zxh404.vscode-proto3":                 { "supported": "limited" }
    },
    "python.defaultInterpreterPath": "python",
    "python.venvFolders": ["venv"],
    "python.venvPath": ".",
    "jupyter.askForKernelRestart": true,
    "jupyter.decorateCells": true,
    "notebook.cellFocusIndicator": "gutter",
    "notebook.showCellStatusBar": "visible",
    "notebook.cellToolbarLocation": { "default": "right" },
    "notebook.cellToolbarVisibility": "hover",
    "notebook.outline.showCodeCells": true,
    "notebook.compactView": true,
    "notebook.insertToolbarLocation": "hidden",
    "notebook.lineNumbers": "on",
    "notebook.editorOptionsCustomizations": {},
    "notebook.dragAndDropEnabled": true,
    "notebook.globalToolbar": false,
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.default.compilerPath": "g++",
    "C_Cpp.default.includePath": [
        "${default}",
        "${VCPKG_ROOT}/installed/x64-mingw-static/include",
        "${VULKAN_SDK}/include"
    ],
    "cmake.cmakePath": "",
    "cmake.configureOnOpen": false,
    "cmake.configureOnEdit": false,
    "cmake.configureSettings": {
        "CMAKE_TOOLCHAIN_FILE": "${VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake"
    },
    "cmake.generator": "MinGW Makefiles",
    "arduino.path": "",
    "arduino.commandPath": "arduino_debug.exe",
    "arduino.logLevel": "info",
    "arduino.enableUSBDetection": false,
    "arduino.disableTestingOpen": true,
    "arduino.skipHeaderProvider": true,
    "arduino.additionalUrls": [
        "http://arduino.esp8266.com/stable/package_esp8266com_index.json"
    ],
    "arduino.defaultBaudRate": 115200
}